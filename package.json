{
  "name": "faqtor",
  "version": "0.0.3",
  "description": "Faqtor build automation tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/faqtor/faqtor"
  },
  "license": "MIT",
  "keywords": [
    "build",
    "make",
    "task",
    "build automation"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:es": "tsc -p build/tsconfig.es.json && mv ./dist/index.js ./dist/index.es.js",
    "build:cjs": "tsc -p build/tsconfig.cjs.json && rimraf ./build/faqtor.js && cp ./dist/index.js ./build/faqtor.js",
    "build": "npm run clean:dist && npm run build:es && npm run build:cjs",
    "clean:dist": "rimraf ./dist",
    "clean:all": "rimraf ./dist ./node_modules"
  },
  "author": "Osman Bineev",
  "devDependencies": {
    "cash-cp": "^0.2.0",
    "cash-mv": "^0.2.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.18",
    "@types/string-argv": "^0.1.0",
    "@types/which": "^1.3.1",
    "glob": "^7.1.3",
    "resolve-bin": "^0.4.0",
    "string-argv": "^0.1.1",
    "which": "^1.3.1"
  }
}
