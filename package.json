{
  "name": "faqtor",
  "version": "0.2.9",
  "description": "Faqtor build automation tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/faqtor/faqtor"
  },
  "license": "MIT",
  "keywords": [
    "build",
    "make",
    "task",
    "build automation"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:es": "tsc -p build/tsconfig.es.json && shx mv ./dist/index.js ./dist/index.es.js",
    "build:cjs": "tsc -p build/tsconfig.cjs.json && rimraf ./build/faqtor.js && shx cp ./dist/index.js ./build/faqtor.js",
    "build": "npm run clean && npm run build:es && npm run build:cjs",
    "clean": "rimraf ./dist",
    "wipe": "rimraf ./dist ./node_modules ./coverage",
    "test": "jest --coverage --env=node"
  },
  "author": "Osman Bineev",
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "shx": "^0.3.2",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^13.1.8",
    "@types/string-argv": "^0.3.0",
    "@types/which": "^1.3.2",
    "chalk": "^3.0.0",
    "glob": "^7.1.6",
    "string-argv": "^0.3.1",
    "which": "^2.0.2"
  }
}
